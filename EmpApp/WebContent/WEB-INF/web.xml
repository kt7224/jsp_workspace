<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">

	<servlet>
		<servlet-name>EmpList</servlet-name>
		<servlet-class>com.sds.emp.EmpList</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>EmpList</servlet-name>
		<url-pattern>/list</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EmpRegist</servlet-name>
		<servlet-class>com.sds.emp.EmpRegist</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>EmpRegist</servlet-name>
		<url-pattern>/regist</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>EmpUpdate</servlet-name>
		<servlet-class>com.sds.emp.EmpUpDate</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>EmpUpdate</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>EmpRemove</servlet-name>
		<servlet-class>com.sds.emp.EmpRemove</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>EmpRemove</servlet-name>
		<url-pattern>/remove</url-pattern>
	</servlet-mapping>	
</web-app>